{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=development nodemon src/main.ts",
    "start:prd": "cross-env NODE_ENV=production node dist/main.js",
    "test": "jest",
    "test:watch": "jest --watchAll --colors --verbose=false --silent=false",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "test:all": "jest && jest --config ./e2e/jest-e2e.json",
    "build": "tsc",
    "serve": "node ./dist/main.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "schema:update": "pnpm run typeorm schema:drop -d src/database/database.ts && pnpm run typeorm schema:sync -d src/database/database.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1"
  }
}
